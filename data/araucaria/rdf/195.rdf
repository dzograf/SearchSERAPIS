    <!-- http://www.aifdb.org/nodes/1823 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/1823">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>The comments do not represent a breach of privilege</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/1828"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/1827"/>
