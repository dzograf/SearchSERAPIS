        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>I have accordingly disallowed the notice of question of privilege</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/1827"/>
        <aif:creationDate>2012-04-12 18:23:35</aif:creationDate>
