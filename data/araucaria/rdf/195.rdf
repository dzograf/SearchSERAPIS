    <!-- http://www.aifdb.org/nodes/563 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/563">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>On a perusal of the comments of the Ministers in the matter I am satisfied that there has been no misleading of the House by them as alleged by the Member.</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/567"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/1828"/>
