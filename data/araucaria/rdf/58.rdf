        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>I have to say no to war.</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/612"/>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/5120"/>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/5122"/>
