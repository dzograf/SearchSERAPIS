    <!-- http://www.aifdb.org/nodes/886 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/886">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>Britain&#039;s military standing with America currently so high will suffer unless it can maintain some sort of equivalence with the American military machine now so preponderant.</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/903"/>
