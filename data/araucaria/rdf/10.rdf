        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>All the Member States in the European Union are signatories to the ECHR</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/281"/>
        <aif:creationDate>2012-04-12 18:18:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/279 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/279">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>If member states of the EU are signatories to the ECHR and signatories to the ECHR are obliged to secure to everyone within their jurisdiction the rights and freedoms it defines then - if someone is within the jurisdiction of an EU state he or she is entitled to the protection of the ECHR.</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/281"/>
        <aif:creationDate>2012-04-12 18:18:26</aif:creationDate>
    </NamedIndividual>

