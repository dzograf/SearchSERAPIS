        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>The Bowers court&#039;s initial statement demeans the individual put forward</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/992"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/991"/>
