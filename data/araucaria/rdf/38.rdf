        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>If North Koreans are picked up in China and returned to North Korea they are in danger of being persecuted.</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/486"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/485"/>
        <aif:creationDate>2012-04-12 18:19:04</aif:creationDate>
    </NamedIndividual>

