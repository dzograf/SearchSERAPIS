        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>China is not fulfilling its obligations under the UN Refugee Convention.</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/485"/>
        <aif:creationDate>2012-04-12 18:19:04</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/479 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/479">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>under the UN Refugee Convention to which it is a party China is obligated not to push back asylum seekers in danger of persecution</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/485"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/1714"/>
