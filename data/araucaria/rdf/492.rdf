    <!-- http://www.aifdb.org/nodes/4423 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/4423">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>There is not need to exchange blood for oil</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/4435"/>
        <aif:creationDate>2012-04-12 18:32:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/4424 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/4424">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>Before the first Gulf War two Nobel laureates in economics-Milton Friedman on the right and James Tobin on the left-stated that no war for oil was needed.</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/4436"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/4433"/>
        <aif:creationDate>2012-04-12 18:32:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/4425 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/4425">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>Oil accounts for between 65 and 95 percent of the exports of Persian Gulf nations. In contrast oil makes up only about 7 percent of U.S. imports</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/4434"/>
        <aif:creationDate>2012-04-12 18:32:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/4426 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/4426">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>If economists think that there is no need to exchange blood for oil there is not need to do that</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/4435"/>
        <aif:creationDate>2012-04-12 18:32:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/4427 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/4427">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>But is the conventional wisdom correct that the United States needs to exchange blood for oil? Many economists don t think so.</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/4433"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/4435"/>
        <aif:creationDate>2012-04-12 18:32:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/4428 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/4428">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>The goal is to get the oil from the Persian Gulf countries</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/4436"/>
        <aif:creationDate>2012-04-12 18:32:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/4429 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/4429">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>The best means to achieve the goal if pacific means are not available is to fight a war against them</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/4436"/>
        <aif:creationDate>2012-04-12 18:32:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/4430 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/4430">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>Thus most states whether their governments are friendly to the United States or not have a huge incentive to export oil into the world market.</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/4437"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/4436"/>
        <aif:creationDate>2012-04-12 18:32:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/4431 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/4431">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>the Persian Gulf countries need to sell oil more than the United States needs to buy it.</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/4434"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/4437"/>
        <aif:creationDate>2012-04-12 18:32:27</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/4432 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/4432">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>Selling oil to U.S. is the best means to fulfil the need to sell oil</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/4437"/>
        <aif:creationDate>2012-04-12 18:32:27</aif:creationDate>
