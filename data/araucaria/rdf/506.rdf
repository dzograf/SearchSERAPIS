        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>Let&#039;s concede for the sake of argument that we are all in some sense sinners</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/4650"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/4649"/>
