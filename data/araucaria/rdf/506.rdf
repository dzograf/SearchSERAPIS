        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>But the doctrine of Hell requires that most of us sinners will suffer eternal torment.</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/4653"/>
        <aif:creationDate>2012-04-12 18:33:10</aif:creationDate>
