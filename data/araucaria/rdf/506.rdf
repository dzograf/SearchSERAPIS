        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>Yet if the doctrine of hell is correct I shall be keeping Hitler company in Hell.</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/4653"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/4652"/>
        <aif:creationDate>2012-04-12 18:33:10</aif:creationDate>
    </NamedIndividual>

