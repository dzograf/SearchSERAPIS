    <!-- http://www.aifdb.org/nodes/3032 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/3032">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>The greater goods defense can easily lead to a kind of cost benefit analysis which is deeply repugnant to our moral sense</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/3044"/>
        <aif:creationDate>2012-04-12 18:27:50</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/3033 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/3033">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>Some distributions just aren&#039;t fair.</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/3045"/>
