        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>Strategy reorganised itself around a balance of terror Mutually Assured Destruction.</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/1731"/>
