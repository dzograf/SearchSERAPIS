        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/4310"/>
        <aif:creationDate>2012-04-12 18:32:02</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/4300 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/4300">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>typically atheists have said that the universe is eternal and that&#039;s all. But surely this is unreasonable</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/4311"/>
        <aif:creationDate>2012-04-12 18:32:02</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/4301 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/4301">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
