    <!-- http://www.aifdb.org/nodes/4295 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/4295">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>mathematicians recognize that the idea of an actually infinite number of things leads to self-contradictions</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/4308"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/4307"/>
