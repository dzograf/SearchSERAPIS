    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/4296 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/4296">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>For example what is infinity minus infinity?</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/4308"/>
        <aif:creationDate>2012-04-12 18:32:02</aif:creationDate>
