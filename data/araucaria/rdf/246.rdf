    <!-- http://www.aifdb.org/nodes/2312 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/2312">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>People that believe that the Italian South is inferior are shortshighted and narrow-minded</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/2331"/>
        <aif:creationDate>2012-04-12 18:25:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/2313 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/2313">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>The Italian South is  unquestionably inferior economically to the North but economic prosperity is not  necessarily a true indicator of the quality of a society s life.</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/2327"/>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/2329"/>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/2330"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/2326"/>
        <aif:creationDate>2012-04-12 18:25:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/2314 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/2314">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>While materially  24  impoverished the South has produced many of Italy s greatest men of letters and affairs.</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/2328"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/2327"/>
        <aif:creationDate>2012-04-12 18:25:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/2315 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/2315">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>To cite only a few of her political thinkers of note she can claim Vico Cuoco Mosca  Croce and Dorso.</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/2328"/>
        <aif:creationDate>2012-04-12 18:25:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/2316 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/2316">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>She also remains the land of the wondrous scenic beauties described  by Goethe in his Italienische Reise</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/2329"/>
        <aif:creationDate>2012-04-12 18:25:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/2317 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/2317">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>as well as of architectural marvels like the Baroque  churches of Apulia</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/2330"/>
        <aif:creationDate>2012-04-12 18:25:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/2318 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/2318">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>It would be extremely shortsighted to view the South as simply or  even primarily a place where poverty illiteracy poor soil earthquakes brigandage and  disease abound</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/2332"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/2331"/>
        <aif:creationDate>2012-04-12 18:25:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/2319 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/2319">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>Italian South is considered inferior a land of poverty and brigandage</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/2333"/>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/2334"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/2331"/>
        <aif:creationDate>2012-04-12 18:25:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/2320 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/2320">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>The Italian South is incredibly rich of culture and has an high quality of life</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/2326"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/2332"/>
        <aif:creationDate>2012-04-12 18:25:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/2321 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/2321">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>If the stereotype that Italian South is inferior is open-minded and correct the South is lacking in quality of life and culture</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/2332"/>
        <aif:creationDate>2012-04-12 18:25:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/2322 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/2322">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>The Italian South is economically and culturally different from the North</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/2333"/>
        <aif:creationDate>2012-04-12 18:25:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/2323 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/2323">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>Different is often taken to mean inferior by insensitive observers both Italian and foreign.</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/2333"/>
        <aif:creationDate>2012-04-12 18:25:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/2324 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/2324">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>In much of the recent literature in the social sciences southern Italy is viewed as an  underdeveloped country in comparison with its northern counterpart</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/2334"/>
        <aif:creationDate>2012-04-12 18:25:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/2325 -->
