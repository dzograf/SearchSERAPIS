        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>It is advanced the claim of the existence of a creator</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/4125"/>
        <aif:creationDate>2012-04-12 18:31:27</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/4119 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/4119">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>it invokes an entity unknown to science.</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/4123"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/4126"/>
        <aif:creationDate>2012-04-12 18:31:27</aif:creationDate>
    </NamedIndividual>

