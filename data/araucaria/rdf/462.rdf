    <!-- http://www.aifdb.org/nodes/4115 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/4115">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>The idea of a pre-existing creator is refuted by scientists</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/4126"/>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/4127"/>
        <aif:creationDate>2012-04-12 18:31:27</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/4116 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/4116">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>Suppose that a skeptic argues that evidence for biological creation by natural selection is obviously lacking and that in the circumstances we ought to give serious consideration to the possibility that the development of life required some input from a pre-existing purposeful creator.</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/4123"/>
        <aif:creationDate>2012-04-12 18:31:27</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/4117 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/4117">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>it suggests the possibility that this creator may have communicated in some way with humans.</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/4125"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/4124"/>
        <aif:creationDate>2012-04-12 18:31:27</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/4118 -->
