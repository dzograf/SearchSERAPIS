        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>The June 30th transfer of sovereignty is an essential commitment of our strategy</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/3149"/>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/3150"/>
