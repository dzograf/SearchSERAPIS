    <!-- http://www.aifdb.org/nodes/3137 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/3137">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>After decades under the tyrant they are also reluctant to trust authority.</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/3146"/>
        <aif:creationDate>2012-04-12 18:28:10</aif:creationDate>
