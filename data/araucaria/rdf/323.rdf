        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>Iraqis are proud people who resent foreign control of their affairs just as we would</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/3145"/>
        <aif:creationDate>2012-04-12 18:28:10</aif:creationDate>
