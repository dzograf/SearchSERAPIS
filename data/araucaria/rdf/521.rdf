        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>Americans are trying to seek recognition as an occupying power</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/4780"/>
