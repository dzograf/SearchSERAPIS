    <!-- http://www.aifdb.org/nodes/744 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/744">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>If an attack causes more death injury and phsychological and economic damage than others in a series of attacks it is by definition the deadliest attack in the series</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/754"/>
        <aif:creationDate>2012-04-12 18:19:53</aif:creationDate>
