        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>This was the latest and deadliest of several recent assaults.</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/754"/>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/2033"/>
