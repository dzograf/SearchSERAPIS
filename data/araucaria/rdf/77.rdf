    <!-- http://www.aifdb.org/nodes/743 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/743">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>Several recent attacks associated with Bin Laden suffice to indicate that Al Qaeda and its allies have survived and are regrouping.</aif:claimText>
