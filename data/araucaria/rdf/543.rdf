        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/5040"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/5039"/>
        <aif:creationDate>2012-04-12 18:34:27</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/5028 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/5028">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>But how could anyone prove such a thing?  Again it is pure speculation.</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/5039"/>
        <aif:creationDate>2012-04-12 18:34:27</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/5029 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/5029">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
