        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/5034"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/5036"/>
        <aif:creationDate>2012-04-12 18:34:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/5020 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/5020">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>If God is all good than He would create a world without evil.</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/5035"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/5036"/>
        <aif:creationDate>2012-04-12 18:34:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/5021 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/5021">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>Therefore evil should not exis</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/5036"/>
        <aif:creationDate>2012-04-12 18:34:26</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/5022 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/5022">
