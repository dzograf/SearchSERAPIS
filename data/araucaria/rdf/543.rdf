        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/5040"/>
        <aif:creationDate>2012-04-12 18:34:27</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/5030 -->
