    <!-- http://www.aifdb.org/nodes/1074 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/1074">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>His suggestion is disturbing unworkable and potentially dangerous.</aif:claimText>
