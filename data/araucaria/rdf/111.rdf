    <!-- http://www.aifdb.org/nodes/1019 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/1019">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>The evidence  shows there is no reasonable case for  such exemptions.</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/1026"/>
        <aif:creationDate>2012-04-12 18:20:45</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/1020 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/1020">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>The environmental  laws already allow the Department of  Defense to apply for exemptions on a  case-by-case basis if they really need  it.</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/1026"/>
        <aif:creationDate>2012-04-12 18:20:45</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/1021 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/1021">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>There is no evi-dence  that the military has ever been  refused an exemption from laws that  were necessary and that they sought it.</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/1026"/>
        <aif:creationDate>2012-04-12 18:20:46</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/1022 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/1022">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>If the evidence is that environmental laws have not affected military readiness the military can apply for exemptions from such laws where necessary and there is no evidence that the military has ever been refused such an exemption then the evidence shows that there is no reasonable case for the military to be exempted from environmental laws</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/1026"/>
        <aif:creationDate>2012-04-12 18:20:46</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/1023 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/1023">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>The evidence is that environmental laws have not affected military readiness</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/1027"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/1026"/>
        <aif:creationDate>2012-04-12 18:20:46</aif:creationDate>
    </NamedIndividual>

