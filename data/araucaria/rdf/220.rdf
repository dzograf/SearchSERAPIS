    <!-- http://www.aifdb.org/nodes/1056 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/1056">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>It has tried to proceed in accordance with the Constitution</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/1066"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/2073"/>
        <aif:creationDate>2012-04-12 18:20:52</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/1057 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/1057">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>If the government&#039;s interests in not allowing Moussaoui to talk to Binalshibi are real and there is no question that the defense could have access to such a witness according to the Constitution then if it has tried to proceed according to the Constitution doing so will have caused problems</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/1067"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/2074"/>
        <aif:creationDate>2012-04-12 18:20:52</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/1058 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/1058">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>Under normal circumstances there would be no question that the defense could have access to such a witness -- nor constitutionally speaking should there be such question now.</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/1068"/>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/2075"/>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/2272"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/1067"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/2074"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/2271"/>
        <aif:creationDate>2012-04-12 18:20:52</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/1059 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/1059">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>the government&#039;s other interests here are very real.</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/1069"/>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/2076"/>
