    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/432">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>They did not regard it as clear that the 1938 Act would continue to operate in cases to which it applied irrespective of the terms of Part V</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/442"/>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/443"/>
