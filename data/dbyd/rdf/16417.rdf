    <!-- http://www.aifdb.org/nodes/457638 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/457638">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>По моему мнению, медицинский пункт в Малозёмове вовсе не нужен.</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/457642"/>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/457643"/>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/457644"/>
