        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>There is no need for a historical course of study to be boring to anyone.</aif:claimText>
        <aif:creationDate>2020-06-02 18:10:20</aif:creationDate>
    </NamedIndividual>

