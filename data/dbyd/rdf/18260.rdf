        <aif:claimText>Second, history is fascinating.</aif:claimText>
        <aif:creationDate>2020-06-02 18:10:19</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/538818 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/538818">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>No matter what interests you (peace, war, art, sports, technology, popular culture, etc.), history will allow you to learn more about your interests.</aif:claimText>
        <aif:creationDate>2020-06-02 18:10:20</aif:creationDate>
    </NamedIndividual>

