        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>By learning how the structures of society came into being, we can better understand the world we live in.</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/538826"/>
        <aif:creationDate>2020-06-02 18:10:19</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/538816 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/538816">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>There is no other way to make sense of the contemporary world than to revisit the men and women who shaped our society.</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/538826"/>
        <aif:creationDate>2020-06-02 18:10:19</aif:creationDate>
