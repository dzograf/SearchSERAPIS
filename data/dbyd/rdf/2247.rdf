        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>Should the UK implement internet censorship?</aif:claimText>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/47841"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/47843"/>
        <aif:creationDate>2015-03-02 10:03:24</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/47840 -->
    <NamedIndividual rdf:about="&http;www.aifdb.org/nodes/47840">
        <rdf:type rdf:resource="&http;www.arg.tech/aif#I-node"/>
        <aif:claimText>Violates freedom of speech, thus violates basic human rights.</aif:claimText>
        <aif:Conclusion rdf:resource="&http;www.aifdb.org/nodes/47841"/>
        <aif:Premise rdf:resource="&http;www.aifdb.org/nodes/47841"/>
        <aif:creationDate>2015-03-02 10:03:24</aif:creationDate>
    </NamedIndividual>

    <!-- http://www.aifdb.org/nodes/47841 -->
